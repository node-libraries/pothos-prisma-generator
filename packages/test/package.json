{
  "name": "@/test",
  "version": "1.0.7",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint": "eslint ./tests",
    "lint:fix": "eslint --fix ./tests",
    "dev:docker": "docker compose -p pothos-prisma-generator -f docker/docker-compose.yml up -d",
    "prisma:generate": "prisma generate",
    "prisma:format": "prisma format",
    "prisma:migrate": "next-exec -c test -- prisma migrate dev -n migrate",
    "prisma:reset": " next-exec -c test -- prisma migrate reset --schema=prisma/schema.prisma -f",
    "prisma:seed": "next-exec -c test -- tsx bin/seeds",
    "prisma:test": "next-exec -c test -- tsx bin/test",
    "pothos:schema": "tsx bin/createSchema",
    "codegen": "graphql-codegen --config codegen/codegen.json",
    "server": "next-exec -c test -- ts-node-dev -r tsconfig-paths/register bin/server.ts",
    "test:reset": "pnpm run prisma:reset && pnpm run prisma:seed",
    "test": "next-exec -c test -- jest --coverage"
  },
  "devDependencies": {
    "@apollo/server": "^4.11.2",
    "@as-integrations/fastify": "^2.1.1",
    "@graphql-codegen/cli": "^5.0.3",
    "@graphql-codegen/typescript": "^4.1.2",
    "@graphql-codegen/typescript-generic-sdk": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.4.0",
    "@pothos/core": "^4.3.0",
    "@pothos/plugin-prisma": "^4.4.0",
    "@pothos/plugin-prisma-utils": "^1.2.0",
    "@pothos/plugin-scope-auth": "^4.1.1",
    "@prisma/client": "^6.1.0",
    "@types/jest": "^29.5.14",
    "@types/mocha": "10.0.10",
    "@types/traverse": "^0.6.37",
    "cpy-cli": "^5.0.0",
    "eslint": "9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "fastify": "^5.2.0",
    "graphql": "^16.10.0",
    "graphql-tag": "^2.12.6",
    "jest": "^29.7.0",
    "jest-async": "^1.0.7",
    "next-exec": "^1.0.0",
    "pothos-schema-exporter": "^1.0.0",
    "prisma": "^6.1.0",
    "ts-jest": "^29.2.5",
    "ts-jest-resolver": "^2.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "ts-patch": "^3.3.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.0",
    "typescript-transform-paths": "^3.5.3"
  },
  "author": "SoraKumo <info@croud.jp>",
  "repository": "https://github.com/node-libraries/pothos-prisma-generator"
}
